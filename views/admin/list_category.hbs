
<table class="table">
  <thead class="thead-light">
    <tr>
      
      <th scope="col">Category Name</th>
      <th scope="col">Change Status</th>
      
    </tr>
  </thead>
  <tbody>
    {{#each response}}
    <tr>
      
      <td>{{this.categoryname}}</td>
      

     

      <td>
                    
                    <a class="btn btn-success" href="/admin/update-category/{{this._id}}"
                        >Edit
                        
                    </a>
                   
                    <a class="btn btn-danger" href="/admin/delete-category/{{this._id}}"
                        onclick="return confirm('Are you sure you want to delete {{this.categoryname}} ?')">Delete
                       
                        </ion-icon>
                    </a>
                    
                    {{!-- <ion-icon name="close-outline"></ion-icon> --}}
                </td>



    </tr>
    {{/each}}
  </tbody>
</table>