<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Code</th>
            <th scope="col">Discount</th>
            <th scope="col">Limit</th>
             <th scope="col">Change Status</th>



        </tr>
    </thead>
    {{#each response}}
    <tbody>



        <tr>

            <td>{{this.CouponName}}</td>
            <td>{{this.CouponCode}}</td>
            <td>{{this.DiscountPrice}}</td>
            <td>{{this.CouponLimit}}</td>
                <td>
        <a class="btn btn-success" href="/admin/update-coupon/{{this._id}}">Edit

        </a>

        <a class="btn btn-danger" href="/admin/delete-coupon/{{this._id}}"
          onclick="return confirm('Are you sure you want to delete  {{this.CouponName}} ?')">Delete

          </ion-icon>
        </a>
      </td>
           

            



        </tr>
    </tbody>
    {{/each}}
</table>